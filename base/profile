eval $(keychain --eval --agents ssh -Q --quiet id_rsa)

# Add ~/bin to PATH
export PATH="$HOME/bin:$PATH"

# golang
export GOROOT=/usr/local/go

export TERM=xterm
# Default editor is vim and use vi keybindings in the shell
export EDITOR=vim
export ALTERNATE_EDITOR=""
export VISUAL=vim
set -o vi

# custom prompt
export PS1="\[\033[0;36m\]\w\n[\t]$ \[\033[0m\]"
